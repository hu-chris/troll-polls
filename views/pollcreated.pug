extends homelayout

block content
    div(id='createdwrapper' class='text-center')
        div(class='created') Poll Created!
        div(class='text-center')
            a(class='btn btn-primary createdbutton' id='returnbutton' href='/') Return to Homepage
            - var pollroute = pollid;
            a(class='btn btn-primary createdbutton' href='polls?'+pollroute) View Your Poll